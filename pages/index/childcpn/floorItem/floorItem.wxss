/* pages/index/childcpn/floorItem/floorItem.wxss */
.floor-title{
  width: 100%;
  padding: 10rpx;
}
.floor-img{
  overflow: hidden;
  margin-left: 10rpx;
}
.floor-img navigator{
  float: left;
  width: 32%;
}
.floor-img navigator:nth-last-child(-n + 4){
  height: 27.7271207vw;
  border-left: 10rpx solid #fff;
}
.floor-img navigator:nth-child(2), .floor-img navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}

.floor-img navigator image{
  width: 100%;
  height: 100%;
}